Cell[TextData[{
 "quark loop function of \[Rho] two-point function\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    SubscriptBox["\[CapitalSigma]", "q"], "=", 
    RowBox[{"Tr", 
     RowBox[{"{", 
      RowBox[{
       SubscriptBox["ih", "k"], 
       SubscriptBox["\[Gamma]", "\[Mu]"], 
       SuperscriptBox["T", "i"], 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["G", "k"], "q"], "(", "q", ")"}], 
       SubscriptBox["ih", "k"], 
       SubscriptBox["\[Gamma]", "\[Nu]"], 
       SuperscriptBox["T", "l"], 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["G", "k"], "q"], "(", 
        RowBox[{"q", "-", "p"}], ")"}]}], "}"}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"      ", 
    RowBox[{"=", 
     RowBox[{
      RowBox[{"-", 
       FractionBox[
        SuperscriptBox[
         SubscriptBox["h", "k"], "2"], 
        SuperscriptBox[
         SubscriptBox["Z", "q"], "2"]]}], 
      RowBox[{"tr", "(", 
       RowBox[{
        SuperscriptBox["T", "i"], 
        SuperscriptBox["T", "l"]}], ")"}], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox[
         OverscriptBox["G", "_"], "k"], "q"], "(", "q", ")"}], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox[
         OverscriptBox["G", "_"], "k"], "q"], "(", 
       RowBox[{"q", "-", "p"}], ")"}], "tr", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[Gamma]", "\[Mu]"], "(", 
         RowBox[{
          RowBox[{
           RowBox[{"-", 
            SuperscriptBox[
             SubscriptBox["iq", "F"], "\[Sigma]"]}], 
           SubscriptBox["\[Gamma]", "\[Sigma]"]}], "+", 
          SubscriptBox[
           OverscriptBox["m", "_"], "q"]}], ")"}], 
        RowBox[{
         SubscriptBox["\[Gamma]", "\[Nu]"], "(", 
         RowBox[{
          RowBox[{
           RowBox[{"-", 
            SuperscriptBox[
             SubscriptBox[
              RowBox[{"i", "(", 
               RowBox[{"q", "-", "p"}], ")"}], "F"], "\[Rho]"]}], 
           SubscriptBox["\[Gamma]", "\[Rho]"]}], "+", 
          SubscriptBox[
           OverscriptBox["m", "_"], "q"]}], ")"}]}], "}"}]}]}]}], 
   TraditionalForm]}],
  InitializationCell->True,
